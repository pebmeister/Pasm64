                                                  ;*                                          *
                                                  ;*  destroys    a,y                         *
                                                  ;*                                          *
                                                  ;********************************************
                                                  PRNTLN
                                                                  ;
                                                                  ;   get current line number
                                                                  ;   store in bnvar
                                                                  ;
                                                                  ldy #LNOFF
                                                                  lda (NXTLN),y
                                                                  sta BNVAR
                                                                  iny
                                                                  lda (NXTLN),y
                                                                  sta BNVAR+1
