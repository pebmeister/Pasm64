                                                  ;********************************************
                                                  ;*                                          *
                                                  ;* DUMP_BYTE                                *
                                                  ;*                                          *
                                                  ;*  print number in a  in hex               *
                                                  ;*                                          *
                                                  ;*  destroys a                              *
                                                  ;*                                          *
                                                  ;********************************************
                                                  DUMP_BYTE
                                                                  pha
                                                                  lsr
                                                                  lsr
                                                                  lsr
                                                                  lsr
                                                                  jsr DUMP_NYBBLE
                                                                  pla
                                                                  and #$0F
                                                                  ; fall through
                                                  
                                                  ;********************************************
                                                  ;*                                          *
                                                  ;* DUMP_NYBBLE                              *
                                                  ;*                                          *
                                                  ;*  converts the 4 bit nybble in a to       *
                                                  ;*  an ascii character and prints it        *
                                                  ;*                                          *
                                                  ;*  destroys a                              *
                                                  ;*                                          *
                                                  ;********************************************
                                                  DUMP_NYBBLE
                                                                  ora #'0'
                                                                  cmp #'9' + 1
                                                                  bcc +
                                                                  adc #'A' - ('9' + 2)
                                                  +
                                                                  jmp CHROUT
                                                  
                                                  ;********************************************
                                                  ;*                                          *
                                                  ;* DUMP_STR                                 *
                                                  ;*                                          *
                                                  ;*  print string pointed to by y and a      *
                                                  ;*  a low byte y high byte null byte marks  *
                                                  ;*  end of string                           *
                                                  ;*  destroys a,y                            *
                                                  ;*                                          *
                                                  ;********************************************
                                                  DUMP_STR
                                                                  sty DUMPSTR
                                                                  sta DUMPSTR + 1
                                                                  ldy #0
                                                  -
                                                                  lda (DUMPSTR),y
                                                                  beq +
                                                                  jsr CHROUT
                                                                  iny
                                                                  bne -
                                                  +
                                                                  rts
                                                  
                                                  ;********************************************
                                                  ;*                                          *
                                                  ;*  PRNTLN                                  *
                                                  ;*                                          *
