    ; loop test
    .var count, aaa, bbb
    .for count = 1 .To 3 .step 2
        aaa = count
        .while (aaa<6)
            aaa = aaa + 1
            ldx #aaa
            .if (aaa>4)
                brk
                brk
            .else
                bbb = 0
                .repeat
                    nop
                    bbb = bbb + 2
                .until (bbb > 6)
                .do
                    lda #bbb
                    bbb = bbb + 5
                .while bbb < 15
            .endif
        .wend
    .next count

    .macro macroNoParam
